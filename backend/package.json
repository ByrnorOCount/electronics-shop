{
  "name": "online-shop-backend",
  "version": "1.0.0",
  "description": "Express.js backend for Online Shop (API, auth, DB)",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "migrate": "knex migrate:latest --knexfile ./database/knexfile.js",
    "rollback": "knex migrate:rollback --knexfile ./database/knexfile.js",
    "seed": "knex seed:run --knexfile ./database/knexfile.js",
    "make:migration": "knex migrate:make --knexfile ./database/knexfile.js",
    "make:seed": "knex seed:make --knexfile ./database/knexfile.js"
  },
  "knex": {
    "knexfile": "./database/knexfile.js"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.19.2",
    "express-session": "^1.18.2",
    "google-auth-library": "^10.5.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "nodemailer": "^7.0.10",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.16.3",
    "stripe": "^19.1.0",
    "tiny-csrf": "^1.1.6",
    "vnpay": "^2.4.4"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}